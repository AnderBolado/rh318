sudo apt-get install --yes bind9 isc-dhcp-server
sudo sysctl net.ipv4.ip_forward=1
sudo iptables -t nat -A POSTROUTING -o enp0s3 -j MASQUERADE
sudo iptables -t nat -A PREROUTING -p tcp -m tcp --dport 443  -j DNAT --to-destination 192.168.1.30:443
