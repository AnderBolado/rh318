sudo yum install -y bind bind-utils dhcp && sudo systemctl enable dhcpd named && sudo firewall-cmd --add-service=dhcp --add-service=dns --zone=internal --permanent&& sudo firewall-cmd --change-interface=ens33 --zone=internal --permanent && sudo firewall-cmd --change-interface=ens32 --zone=external --permanent&& sudo firewall-cmd --reload
sudo apt-get install --yes bind9 isc-dhcp-server
sudo sysctl net.ipv4.ip_forward=1 && sudo iptables -t nat -A POSTROUTING -o enp0s3 -j MASQUERADE && sudo iptables -t nat -A PREROUTING -p tcp -m tcp --dport 443  -j DNAT --to-destination 192.168.1.30:443
